# Single Inheritance
class Animal:
    def speak(self):
        print("Animal speaks")

class Dog(Animal):
    def bark(self):
        print("Dog barks")


def main():
    print("Single Inheritance:")
    dog = Dog()
    dog.speak()  # Inherited method from Animal
    dog.bark()  # Dog's own method

if __name__ == "__main__":
    main()
